{
  "name": "nuxt-demo",
  "version": "1.0.0",
  "description": "My sweet Nuxt.js project",
  "author": "harry",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "dev-test": "cross-env NODE_ENV=development TEST=true nodemon server/index.js --watch server",
    "build": "nuxt build",
    "build-test": "cross-env TEST=true nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "start-test": "cross-env NODE_ENV=production TEST=true node server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.0.0",
    "@nuxtjs/proxy": "^1.3.3",
    "@nuxtjs/style-resources": "^0.1.2",
    "cookie-universal-nuxt": "^2.0.14",
    "cross-env": "^5.2.0",
    "cube-ui": "^1.12.13",
    "koa": "^2.5.2",
    "nuxt": "^2.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "eslint": "^5.0.1",
    "eslint-config-prettier": "^3.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-vue": "^4.0.0",
    "node-sass": "^4.11.0",
    "nodemon": "^1.11.0",
    "prettier": "1.14.3",
    "sass-loader": "^7.1.0"
  },
  "transformModules": {
    "cube-ui": {
      "transform": "cube-ui/lib/${member}",
      "kebabCase": true,
      "style": {
        "ignore": [
          "create-api",
          "better-scroll",
          "locale"
        ]
      }
    }
  }
}
